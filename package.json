{
  "name": "fireproxy",
  "version": "0.0.1",
  "description": "FireProxy HTTP/WS proxy module",
  "main": "dist/index.js",
  "author": "Vladyslav Tkachenko <vlad@fireblink.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "nyc --reporter=html --reporter=text mocha"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "axios": "^0.27.2",
    "mocha": "^10.0.0",
    "mocha-typescript": "^1.1.17",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nyc": {
    "cache": false,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/**/index.ts",
      "src/interfaces/*.ts",
      "**/*.d.ts",
      "coverage/**"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src",
      "dist/src"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    }
  ],
  "dependencies": {}
}
